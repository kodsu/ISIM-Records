s += b[j+1] + b[--j]; 

t := j+1; 
t := 4*t; 
t := b[t]; 
j := j-1; 
t2 := 4*j; 
t2 := b[t2]; 
t3 := t + t2; 
s := s + t3; 

a[i++] -= *b * (c[j*2] + 1); 

t1 := *b; 
t2 := j*2; 
t2 := t2 * 4; 
t2 := c[t2]; 
t2 := t2 + 1; 
t3 := t1 * t3;  
ind = 4*i; 
t4 := a[ind]; 
t4 := t4 - t3;  
a[ind] := t4; 
i := i+1; 

